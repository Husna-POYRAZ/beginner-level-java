import java.io.File;
import java.io.FileWriter;

public class App {
    public static void main(String[] args) throws Exception {
        try {
            File output = new File("./asset/output.txt");
            FileWriter fw = new FileWriter(output);
            fw.write("Merhaba benim adım Hüsna.");
            fw.close();
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}
