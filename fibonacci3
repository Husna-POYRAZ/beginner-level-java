public class App {
    public static int fib(int x) {
        if(x == 0 || x == 1 ) return 1;
        return fib(x - 2) + fib(x - 1);
    }
    public static void main(String[] args) throws Exception {
        for(int i = 0; i < 10; i++) {
            System.out.println(fib(i));
        }
        System.out.println("Fibonacci 10 = " + fib(10));
    }
}
