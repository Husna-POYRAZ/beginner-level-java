import java.io.File;
import java.io.FileReader;

public class App {
    public static void main(String[] args) throws Exception {
        File f = new File("./asset/input.txt");
        FileReader fr = new FileReader(f);
        int c = 0;
        try {
            while((c = fr.read()) != -1) {
                char character = (char) c;
                System.out.println(character); 
            }
            fr.close();
        } catch (Exception e) {
            e.printStackTrace();
        }

    }
}
