public class App {
    // f(x)=5x+10
    static int f(int x) {
        return 5*x+10;
    }

    static int g(int x, int y) {
        return f(y+2*x);
    }

    static void write() {
        System.out.println("Hello !");
    }
    public static void main(String[] args) throws Exception {
         System.out.println(f(10));
         System.out.println(g(3,5));
         write();
    }
}
